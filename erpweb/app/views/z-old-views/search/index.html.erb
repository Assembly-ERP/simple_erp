<h1>Search Results</h1>

<h2>Products</h2>
<ul>
  <% @products.each do |product| %>
    <li>
      <%= link_to product.name, product_path(product) %>
    </li>
  <% end %>
</ul>

<h2>Parts</h2>
<ul>
  <% @parts.each do |part| %>
    <li>
      <%= link_to part.name, part_path(part) %>
    </li>
  <% end %>
</ul>
